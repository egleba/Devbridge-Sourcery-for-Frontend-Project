    "@babel/preset-env": "7.6.3",
    "autoprefixer": "9.6.4",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.0.5",
    "css-loader": "3.2.0",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "extract-loader": "3.1.0",
    "file-loader": "4.2.0",
    "handlebars": "4.5.3",
    "handlebars-loader": "1.7.1",
